#===================================================
# Set
record(ao, "$(P){$(Q)-Ax:$(AX)}:$(CMD)") {
  field(DESC, "Axis $(AX) $(CMD)")
  field(DTYP, "stream")
  field(FLNK, "$(P){$(Q)-Ax:$(AX)}:$(CMD):RBV")
  field(OUT, "@pump33dds.proto setRateAx($(P), $(Q), $(AX), $(CMD)) $(PORT)")
  field(PREC, "0")
  field(SDIS, "$(P){$(Q)}:DISABLE")
}

