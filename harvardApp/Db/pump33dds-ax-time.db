#===================================================
# Set
record(ao, "$(P){$(Q)-Ax:$(AX)}:$(CMD)") {
  field(DESC, "Axis $(AX) $(CMD)")
  field(DTYP, "stream")
  field(FLNK, "$(P){$(Q)-Ax:$(AX)}:$(CMD):RBV")
  field(OUT, "@pump33dds.proto setTimeAx($(P), $(Q), $(AX), $(CMD)) $(PORT)")
  field(PREC, "0")
  #field(SDIS, "$(P){$(Q)-Ax:$(AX)}:DISABLE")
}

#===================================================
# Get
record(ai, "$(P){$(Q)-Ax:$(AX)}:$(CMD):RBV") {
  field(DESC, "Axis $(AX) $(CMD) readback")
  field(DTYP, "stream")
  field(INP, "@pump33dds.proto getTimeAx($(P), $(Q), $(AX), $(CMD)) $(PORT)")
  field(PREC, "0")
  #field(SDIS, "$(P){$(Q)-Ax:$(AX)}:DISABLE")
}

