#===================================================
# Get rate.
record(ai, "$(P){$(Q)-Ax:B}:RATE:RBV") {
  field(DTYP, "stream")
  field(INP, "@pump33dds.proto getStatusRateAxB($(P), $(Q), $(CMD)) $(PORT)")
  field(PREC, "0")
  field(SCAN, "I/O Intr")
  #field(SDIS, "$(P){$(Q)-Ax:B}:DISABLE")
}


