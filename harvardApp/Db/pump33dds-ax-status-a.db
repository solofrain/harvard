#===================================================
# Get rate.
record(ai, "$(P){$(Q)-Ax:A}:RATE:RBV") {
  field(DTYP, "stream")
  field(INP, "@pump33dds.proto getStatusRateAxA($(P), $(Q), $(CMD)) $(PORT)")
  field(PREC, "0")
  field(SDIS, "$(P){$(Q)-Ax:A}:DISABLE")
}


