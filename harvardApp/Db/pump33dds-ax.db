record(ao, "$(P){$(Q)-Ax:$(AX)}:FORCE") {
  field(DESC, "Force")
  field(DTYP, "stream")
  field(FLNK, "$(P){$(Q)-Ax:$(AX)}:FORCE:RBV")
  field(OUT, "@pump33dds-ax.proto setIntAx($(P)$(Q) force $(AX)) $(PORT)")
  field(PREC, "0")
  field(EGU, "%")
  field(DRVH, "100")
  field(DRVL, "0")
#  field(SDIS, "$(P)$(Q):DISABLE")
}

record(ai, "$(P){$(Q)-Ax:$(AX)}:FORCE:RBV") {
  field(DESC, "Force Readback")
  field(DTYP, "stream")
  field(INP, "@pump33dds-ax.proto getIntAx($(P)$(Q) force $(AX)) $(PORT)")
  field(PREC, "0")
  field(EGU, "%")
#  field(SDIS, "$(P)$(Q):DISABLE")
}

