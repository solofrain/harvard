#====================================================
# Ax status

record(mbbi, "$(P){$(Q)-Ax:$(AX)}STATUS") {
  field(DESC, "Pump Status")
  field(ZRVL, "0")
  field(ZRST, "Idle")
  field(ONVL, "1")
  field(ONST, "Infusing")
  field(TWVL, "2")
  field(TWST, "Withdrawing")
  field(THVL, "3")
  field(THST, "Stalled")
  field(FRVL, "4")
  field(FRST, "Target Reached")
  field(FVVL, "5")
  field(FVST, "Unknown")
  field(SDIS, "$(P){$(Q)}DISABLE")
}

